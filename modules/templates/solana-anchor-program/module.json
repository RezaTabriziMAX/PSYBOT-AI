{
  "name": "solana-anchor-program",
  "version": "0.1.0",
  "description": "Template for a Solana Anchor program module with CI-friendly layout.",
  "kind": "template",
  "entryFile": "README.md",
  "runtime": {
    "kind": "native",
    "version": "anchor>=0.30"
  },
  "inputs": {},
  "outputs": {},
  "files": {
    "include": [
      "src/**",
      "README.md",
      "package.json",
      "tsconfig.json",
      "Cargo.toml",
      "Anchor.toml",
      "tests/**",
      "test/**"
    ],
    "exclude": [
      "node_modules/**",
      "target/**",
      "dist/**",
      ".git/**"
    ]
  }
}
